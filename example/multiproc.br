process.fork 'proc1';

process.send proc1 'print "aoba"';
process.send proc1 'print "111"';
process.send proc1 'print "44"';
process.send proc1 'return "aobaaaaa"';
function 'fn' 'print "proc1 returned:" (process.receive proc1);';
repeat 4 'fn';

process.send proc1 'print "aoba"';
print (process.receive proc1);
return 0;